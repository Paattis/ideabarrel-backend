{
  "name": "ideabarrel-backend",
  "version": "1.0.0",
  "description": "Backend for the App for the course TX00CK67-3010",
  "main": "dist\\index.js",
  "scripts": {
    "test": "npx jest --forceExit --detectOpenHandles --watchAll --maxWorkers=1",
    "start": "ts-node src/index.ts",
    "dev": "nodemon",
    "format" : "npx prettier src __tests__ --write",
    "checkCodeStyle" : "npx prettier src __tests__ --check",
    "build": "npx tsc && npx tslint --project . && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Paattis/ideabarrel-backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Paattis/ideabarrel-backend/issues"
  },
  "homepage": "https://github.com/Paattis/ideabarrel-backend#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.1",
    "jest": "^29.2.2",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/supertest": "^2.0.12",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "npx": "^10.2.2",
    "supertest": "^6.3.1",
    "tslint": "^6.1.3"
  }
}
